{
  "name": "@dcdavidev/cspell-config",
  "description": "A collection of configuration files for cspell",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcdavidev/cspell",
    "directory": "packages/cspell-config"
  },
  "keywords": [
    "cspell",
    "cspell-config",
    "spelling",
    "spell-checker"
  ],
  "author": "Davide Di Criscito <dcdavidev@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dcdavidev/cspell/issues"
  },
  "homepage": "https://github.com/dcdavidev/cspell/blob/main/packages/cspell-config/README.md",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./dicts": "./dicts/*.txt",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dicts",
    "LICENSE"
  ],
  "dependencies": {
    "tslib": "^2.3.0",
    "@cspell/cspell-types": "^9.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
